<?xml version = "1.0"?>
<XML ID = "EQInterfaceDefinitionLanguage">
	<Schema xmlns = "EverQuestData" xmlns:dt = "EverQuestDataTypes"/>

	<Button item = "GTMW_StatusButton">
		<ScreenID>GuildStatusButton</ScreenID>
	</Button>

	<Screen item = "GTMW_LabelWnd">
		<ScreenID>GuildLabelWnd</ScreenID>
		<DrawTemplate>WDT_Static</DrawTemplate>
		<Pieces>GTMW_StatusButton</Pieces>
	</Screen>

	<Listbox item = "GTMW_ActiveBenefitList">
		<ScreenID>GuildActiveBenefitList</ScreenID>
		<DrawTemplate>WDT_Static</DrawTemplate>
		<Columns />
		<Columns />
		<Columns />
	</Listbox>

	<Button item = "GTMW_DowngradeButton">
		<ScreenID>GuildDowngradeButton</ScreenID>
	</Button>

	<Screen item = "GTMW_ActiveBenefitArea">
		<DrawTemplate>WDT_Static</DrawTemplate>
		<Pieces>GTMW_ActiveBenefitList</Pieces>
		<Pieces>GTMW_DowngradeButton</Pieces>
	</Screen>

	<Listbox item = "GTMW_BenefitMasterList">
		<ScreenID>GuildBenefitMasterList</ScreenID>
		<DrawTemplate>WDT_Static</DrawTemplate>
		<Columns />
		<Columns />
		<Columns />
		<Columns />
	</Listbox>

	<Button item = "GTMW_UpgradeButton">
		<ScreenID>GuildUpgradeButton</ScreenID>
	</Button>

	<Screen item = "GTMW_BenefitMasterArea">
		<DrawTemplate>WDT_Static</DrawTemplate>
		<Pieces>GTMW_BenefitMasterList</Pieces>
		<Pieces>GTMW_UpgradeButton</Pieces>
	</Screen>

	<HorizontalLayoutBox item = "GTMW_Layout">
		<ScreenID>GuildLayout</ScreenID>
		<DrawTemplate>WDT_Static</DrawTemplate>
		<Pieces>Screen:GTMW_BenefitMasterArea</Pieces>
		<Pieces>Screen:GTMW_ActiveBenefitArea</Pieces>
	</HorizontalLayoutBox>

	<STMLbox item = "GTMW_Description">
		<ScreenID>GuildDescription</ScreenID>
		<DrawTemplate>WDT_Static</DrawTemplate>
	</STMLbox>

	<Button item = "GTMW_SelectedItem">
		<ScreenID>GuildSelectedItem</ScreenID>
	</Button>

	<Label item ="GTMW_ValueLabel">
		<ScreenID>GuildValueLabel</ScreenID>
	</Label>

	<Button item = "GTMW_CoinButton">
		<ScreenID>GuildCoinButton</ScreenID>
	</Button>
	<Button item = "GTMW_DonateButton">
		<ScreenID>GuildDonateButton</ScreenID>
	</Button>

	<Screen item = "GTMW_DonateWnd">
		<ScreenID>GuildDonateWnd</ScreenID>
		<DrawTemplate>WDT_Static</DrawTemplate>
		<Pieces>GTMW_ValueLabel</Pieces>
		<Pieces>GTMW_SelectedItem</Pieces>
		<Pieces>GTMW_DonateButton</Pieces>
		<Pieces>GTMW_CoinButton</Pieces>
	</Screen>

	<Button item = "GTMW_LockButton">
		<ScreenID>LockButton</ScreenID>
	</Button>

	<Screen item = "GuildTributeMasterWnd">
		<DrawTemplate>WDT_Static</DrawTemplate>
		<Pieces>GTMW_Description</Pieces>
		<Pieces>GTMW_LabelWnd</Pieces>
		<Pieces>GTMW_Layout</Pieces>
		<Pieces>GTMW_DonateWnd</Pieces>
		<Pieces>GTMW_LockButton</Pieces>
	</Screen>
</XML>
